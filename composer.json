{
    "name": "coolchi/daterangepicker",
    "description": "A simple Date Range filter for Laravel Nova 4.",
    "keywords": [
        "laravel",
        "nova",
        "date",
        "daterange",
        "filter"
    ],
    "authors": [
        {
            "name": "Roger Rodrigo Paco Jaldin"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Coolchi\\Daterangepicker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Coolchi\\Daterangepicker\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
